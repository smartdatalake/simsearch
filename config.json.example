			"key_column":"1",
			"value_column":"4",
			"search_value":"100000",
			"similarity":"exponential",
			"normalized":"z",
			"decay":"200",
			"weight":"1.0"
		},
		{
			"name":"EmployeesSimSearch"
			"operation":"numerical_topk",
			"input_file":"./data/input.csv",
			"max_lines":"-1",
			"column_delimiter":";",
			"header":"true",
			"key_column":"1",
			"value_column":"3",
			"search_value":"5",
			"similarity":"proportional",
			"normalized":"false",
			"weight":"1.0"
		}
