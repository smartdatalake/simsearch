{
	"algorithm":"threshold",
	"output_file":"./data/output.csv",
	"log_file":"./data/run.log",
	"column_delimiter":";",
	"header":"true",
	"k":"20",
	"queries":[    
		{
			"name":"KeywordSimSearch",
			"operation":"categorical_topk",
			"input_file":"./data/input.csv",
			"max_lines":"-1",
			"set_column":"1",
			"tokens_column":"8",
			"column_delimiter":";",
			"token_delimiter":",",
			"header":"true",
			"search_keywords":"Computer+science,Electronics,Software,E-commerce",
