					"key_column":"article_id",
					"search_column":"persons"
				},
				{
					"operation":"numerical_topk",
					"source":"localPath1",
					"dataset":"sample.csv",
					"separator":",",
					"header":"true",
					"key_column":"article_id",
					"search_column":"timestamp"
				}
			]
